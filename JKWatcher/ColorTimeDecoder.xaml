<Window x:Class="JKWatcher.ColorTimeDecoder"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JKWatcher"
        mc:Ignorable="d"
        Title="ColorTimeDecoder" Height="380" Width="600">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="80"></RowDefinition>
        </Grid.RowDefinitions>
        <WrapPanel x:Name="selectStuffPanel" Background="#999">
            
        </WrapPanel>
        <StackPanel Grid.Row="1">
            <WrapPanel>
                <Label MinWidth="200">Min time:</Label>
                <TextBox Width="200" IsReadOnly="True" x:Name="minTimeTxt"></TextBox>
            </WrapPanel>
            <WrapPanel>
                <Label MinWidth="200">Max time:</Label>
                <TextBox Width="200" IsReadOnly="True" x:Name="maxTimeTxt"></TextBox>
            </WrapPanel>
        </StackPanel>
    </Grid>
</Window>
