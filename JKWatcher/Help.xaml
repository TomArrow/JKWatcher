<Window x:Class="JKWatcher.Help"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JKWatcher"
        xmlns:mdxam="clr-namespace:MdXaml;assembly=MdXaml"
        mc:Ignorable="d"
        Title="Help" Height="600" Width="1000">
    <Grid>
        <TabControl>
            <TabItem Header="configs">
                <mdxam:MarkdownScrollViewer  x:Name="configsReader"  Zoom="100" MaxZoom="200" MinZoom="10"></mdxam:MarkdownScrollViewer>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
