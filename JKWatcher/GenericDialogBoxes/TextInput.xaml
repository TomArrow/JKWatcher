<Window x:Class="JKWatcher.GenericDialogBoxes.TextInput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JKWatcher.GenericDialogBoxes"
        mc:Ignorable="d"
        Title="TextInput" Height="150" Width="400">
    <Grid>
        <StackPanel>
            <TextBlock x:Name="promptTxt">DummyText</TextBlock>
            <TextBox x:Name="responseTxt"></TextBox>
            <Button x:Name="btnOk" Click="btnOk_Click">Ok</Button>
        </StackPanel>
    </Grid>
</Window>
